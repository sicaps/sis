<% @page_title= 'Voir Produit' %>

<%= link_to("<< Back to List", {:action => 'list'}, :class => 'back-link') %>

<div class="subject show">
  <h3>Afficher un enregistrement de la table dim_produits</h3>

  <table summary="Vue détaillée Dim_produits">
    <tr>
      <th>Id</th>
      <td><%= @dim_produit.product_sk %></td>
    </tr>
    <tr>
      <th>Code</th>
      <td><%= @dim_produit.product_code %></td>
    </tr>
    <tr>
      <th>Name</th>
      <td><%= @dim_produit.product_name %></td>
    </tr>
    <tr>
      <th>Catégorie</th>
      <td><%= @dim_produit.product_category %></td>
    </tr>
    <tr>
      <th>Date d'effet</th>
      <td><%= @dim_produit.effective_date %></td>
    </tr>
    <tr>
      <th>Date d'expiration</th>
      <td><%= @dim_produit.expiry_date %></td>
    </tr>


  </table>

</div>
